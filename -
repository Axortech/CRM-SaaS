openapi: 3.0.3
info:
  title: CRM SaaS API
  version: 1.0.0
  description: Interactive documentation for the CRM SaaS platform.
paths:
  /api/v1/activities/:
    get:
      operationId: v1_activities_list
      parameters:
      - in: query
        name: activity_type
        schema:
          type: string
      - in: query
        name: company
        schema:
          type: string
          format: uuid
      - in: query
        name: contact
        schema:
          type: string
          format: uuid
      - in: query
        name: occurred_after
        schema:
          type: string
          format: date-time
      - in: query
        name: occurred_before
        schema:
          type: string
          format: date-time
      - in: query
        name: opportunity
        schema:
          type: string
          format: uuid
      - name: ordering
        required: false
        in: query
        description: Which field to use when ordering the results.
        schema:
          type: string
      - name: page
        required: false
        in: query
        description: A page number within the paginated result set.
        schema:
          type: integer
      - name: per_page
        required: false
        in: query
        description: Number of results to return per page.
        schema:
          type: integer
      - name: search
        required: false
        in: query
        description: A search term.
        schema:
          type: string
      tags:
      - v1
      security:
      - jwtAuth: []
      responses:
        '200':
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/PaginatedActivityList'
          description: ''
    post:
      operationId: v1_activities_create
      tags:
      - v1
      requestBody:
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/Activity'
          application/x-www-form-urlencoded:
            schema:
              $ref: '#/components/schemas/Activity'
          multipart/form-data:
            schema:
              $ref: '#/components/schemas/Activity'
        required: true
      security:
      - jwtAuth: []
      responses:
        '201':
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/Activity'
          description: ''
