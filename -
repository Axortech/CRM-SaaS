      operationId: v1_settings_layouts_list
      parameters:
      - in: query
        name: is_default
        schema:
          type: boolean
      - name: ordering
        required: false
        in: query
        description: Which field to use when ordering the results.
        schema:
          type: string
      - name: page
        required: false
        in: query
        description: A page number within the paginated result set.
        schema:
          type: integer
      - in: query
        name: page_type
        schema:
          type: string
      - name: per_page
        required: false
        in: query
        description: Number of results to return per page.
        schema:
          type: integer
      - name: search
        required: false
        in: query
        description: A search term.
        schema:
          type: string
      - in: query
        name: user
        schema:
          type: string
          format: uuid
      tags:
      - v1
      security:
      - jwtAuth: []
      responses:
        '200':
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/PaginatedLayoutConfigurationList'
          description: ''
    post:
      operationId: v1_settings_layouts_create
      tags:
      - v1
      requestBody:
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/LayoutConfiguration'
          application/x-www-form-urlencoded:
            schema:
              $ref: '#/components/schemas/LayoutConfiguration'
          multipart/form-data:
            schema:
              $ref: '#/components/schemas/LayoutConfiguration'
        required: true
      security:
      - jwtAuth: []
      responses:
        '201':
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/LayoutConfiguration'
          description: ''
